type User {
  id: ID! @id
  nome: String!
	email: String! @unique
	senha: String! 
  updatedAt: DateTime! @updatedAt
  createdAt: DateTime! @createdAt
}

type Client {
  id: ID! @id
  nome: String!
  telefone: String!
  endereco: String!
  user: User!
  updatedAt: DateTime! @updatedAt
  createdAt: DateTime! @createdAt
}

type Order {
  id: ID! @id
  client: Client!
  descricao: String!
  valor: Float!
  updatedAt: DateTime! @updatedAt
  createdAt: DateTime! @createdAt
}